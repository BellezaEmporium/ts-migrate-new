{
  "name": "ts-migrate",
  "version": "0.1.35",
  "description": "A tool for migrating frontend application from JavaScript to TypeScript",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "ts-migrate": "build/cli.js",
    "ts-migrate-full": "bin/ts-migrate-full.sh"
  },
  "keywords": [
    "ts-migrate",
    "typescript",
    "migration",
    "codemod"
  ],
  "files": [
    "build",
    "bin",
    "package.json",
    "README.md",
    "LICENCE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/airbnb/ts-migrate.git",
    "directory": "packages/ts-migrate"
  },
  "bugs": {
    "url": "https://github.com/airbnb/ts-migrate/issues"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext=.js,.jsx,.ts,.tsx --ignore-path ../../.eslintignore",
    "build": "tsc -p tsconfig.json",
    "prepack": "yarn build",
    "clean:linux": "rm -rf build",
    "clean:windows": "del /s /q build",
    "clean": "yarn clean:linux || yarn clean:windows"
  },
  "contributors": [
    {
      "name": "Brie Bunge",
      "url": "https://github.com/brieb"
    },
    {
      "name": "Sergii Rudenko",
      "url": "https://github.com/Rudeg"
    },
    {
      "name": "Elliot Sachs",
      "url": "https://github.com/elliotsa"
    },
    {
      "name": "John Haytko",
      "url": "https://github.com/jjjjhhhhhh"
    },
    {
      "name": "Joe Lencioni",
      "url": "https://github.com/lencioni"
    }
  ],
  "homepage": "https://github.com/airbnb/ts-migrate#readme",
  "license": "MIT",
  "dependencies": {
    "create-jest-runner": "^1.0.0",
    "json10": "^19.0.41",
    "json10-writer": "^19.0.40",
    "ts-migrate-plugins": "^0.1.35",
    "ts-migrate-server": "^0.1.33",
    "updatable-log": "^0.2.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@emotion/core": "^11.0.0",
    "@jest/globals": "^30.2.0",
    "@types/react": "^19.2.0",
    "glob": "^11.0.3",
    "jest": "30.2.0",
    "react": "^19.2.0",
    "rimraf": "^6.0.1",
    "typescript": "5.9.3"
  },
  "gitHead": "7acf6067f15c9bb367cda9c47fcfb4203dcc54f3",
  "peerDependencies": {
    "typescript": ">4.0"
  }
}
